<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Photo:</strong>
    <%= image_tag article.photo, class: "w-9" %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= article.description %>
  </p>

  <p>
    <strong>When went:</strong>
    <%= article.when_went %>
  </p>

  <p>
    <strong>Aventure:</strong>
    <%= article.aventure_id %>
  </p>

</div>
